        "lang-ruby": "#CC342D",
        "lang-rust": "#B84000",
        "lang-swift": "#BC4306",
        "lang-yarn": "#277DA5",
        "prompt": "default",
        "root": "red",
        "terminal-bash": "default",
        "terminal-cmd": "default",
        "terminal-other": "default",
        "terminal-powershell": "default",
        "terminal-pwsh": "default",
        "time": "cyan"
      }
    }
  }
}
