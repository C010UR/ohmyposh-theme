        "lang-java": "#3A75B0",
        "lang-julia": "#4063D8",
        "lang-kotlin": "#774AEC",
        "lang-kubectl": "#316CE6",
        "lang-lua": "#0404CC",
        "lang-node": "#026E00",
        "lang-npm": "#CC0000",
        "lang-nx": "#1976D2",
        "lang-perl": "#4063D8",
        "lang-php": "#4063D8",
        "lang-python": "#326B9B",
        "lang-r": "#2A64B7",
        "lang-ruby": "#CC342D",
        "lang-rust": "#F75500",
        "lang-swift": "#F97532",
        "lang-yarn": "#2C8EBB",
        "prompt": "default",
        "root": "red",
        "terminal-bash": "default",
        "terminal-cmd": "default",
        "terminal-other": "default",
        "terminal-powershell": "default",
        "terminal-pwsh": "default",
        "time": "cyan"
      },
      "light": {
        "code-ko": "red",
        "code-ok": "green",
        "dir-bash": "default",
        "dir-cmd": "default",
        "dir-other": "default",
        "dir-powershell": "default",
        "dir-pwsh": "default",
        "docker": "#0B1F31",
        "git-ahead": "#9C3DDB",
        "git-ahead-behind": "#5D7718",
        "git-behind": "#9C3DDB",
        "git-changed": "#A2220B",
        "git-default": "#CC2C0F",
        "git-staging-changed": "#397500",
        "git-working-changed": "#CC2E33",
        "lang-angular": "#DD0031",
        "lang-cds": "#316A90",
        "lang-cmake": "#5E5E64",
        "lang-crystal": "#4063D8",
        "lang-dart": "#0774A6",
        "lang-dotnet": "#A82FDA",
        "lang-flutter": "#076D9C",
        "lang-go": "#0276AC",
        "lang-haskell": "#5E5086",
        "lang-java": "#3A75B0",
        "lang-julia": "#4063D8",
        "lang-kotlin": "#774AEC",
        "lang-kubectl": "#316CE6",
        "lang-lua": "#0404CC",
