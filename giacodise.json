{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "light-fg": "white",
        "dark-fg": "black",
        "code-ok": "green",
        "code-ko": "red",
        "terminal-pwsh": "#40739E",
        "terminal-powershell": "p:terminal-pwsh",
        "terminal-cmd": "#60717B",
        "terminal-bash": "#A63C40",
        "terminal-other": "p:terminal-cmd",
        "docker": "#0B1F31",
        "docker-fg": "#3A8FE3",
        "git-default": "#FFFB38",
        "git-changed": "#FFEB95",
        "git-ahead-behind": "#C5E478",
        "git-ahead": "#C792EA",
        "git-behind": "#C792EA",
        "git-working-changed": "#7D3638",
        "git-staging-changed": "#095D48",
        "lang-angular": "#D81233",
        "lang-angular-fg": "#FEFEFE",
        "lang-cds": "#A7CAE1",
        "lang-cds-fg": "#1C1C1D",
        "lang-cmake": "#CECECF",
        "lang-cmake-fg": "#1C1C1D",
        "lang-crystal": "#4063D8",
        "lang-crystal-fg": "#FEFEFE",
        "lang-dart": "#03589C",
        "lang-dart-fg": "#FEFEFE",
        "lang-dotnet": "#822CC4",
        "lang-dotnet-fg": "#FEFEFE",
        "lang-flutter": "#60C9F8",
        "lang-flutter-fg": "#FEFEFE",
        "lang-go": "#79D4FD",
        "lang-go-fg": "#FEFEFE",
        "lang-haskell": "#313131",
        "lang-haskell-fg": "#5E5086",
        "lang-java": "#3A75B0",
        "lang-java-fg": "#FEFEFE",
        "lang-julia": "#4063D8",
        "lang-julia-fg": "#FEFEFE",
        "lang-kotlin": "#27282C",
        "lang-kotlin-fg": "#774AEC",
        "lang-kubectl": "#316CE6",
        "lang-kubectl-fg": "#FEFEFE",
        "lang-lua": "#0404CC",
        "lang-lua-fg": "#FEFEFE",
        "lang-node": "#0F9170",
        "lang-yarn": "#0D3B4E",
        "lang-npm": "#700000",
        "lang-node-fg": "#FEFEFE",
        "lang-nx": "#1976D2",
        "lang-nx-fg": "#FEFEFE",
        "lang-perl": "#4063D8",
        "lang-perl-fg": "#FEFEFE",
        "lang-php": "#8892BF",
        "lang-php-fg": "#FEFEFE",
        "lang-python": "#326B9B",
        "lang-python-fg": "#FEFEFE",
        "lang-r": "#C1C2C5",
        "lang-r-fg": "#2A64B7",
        "lang-ruby": "#CC342D",
        "lang-ruby-fg": "#FEFEFE",
        "lang-rust": "#99908A",
        "lang-rust-fg": "#193549",
        "lang-swift": "#F43B26",
        "lang-swift-fg": "#FEFEFE"
    },
    "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "diamond",
                    "foreground": "p:light-fg",
                    "background": "p:terminal-other",
                    "leading_diamond": "\uE0B6",
                    "trailing_diamond": "\uE0B0",
                    "background_templates": [
                        "{{ if eq .Shell \"bash\" }}p:terminal-bash{{end}}",
                        "{{ if eq .Shell \"powershell\" }}p:terminal-powershell{{end}}",
                        "{{ if eq .Shell \"pwsh\" }}p:terminal-pwsh{{end}}",
                        "{{ if eq .Shell \"cmd\" }}p:terminal-cmd{{end}}"
                    ],
                    "properties": {
                        "style": "folder"
                    },
                    "template": " {{ .Path }} "
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:git-default",
                    "foreground": "p:dark-fg",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-ahead-behind{{ end }}",
                        "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
                        "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "\uE725 ",
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "truncate_symbol": "...",
                        "branch_max_length": 25
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <p:git-working-changed>\uEA73 {{ .Working.String }} </>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}|{{ end }}{{ if .Staging.Changed }} <p:git-staging-changed>\uF62B {{ .Staging.String }}</>{{ end }} "
                },
                {
                    "type": "docker",
                    "style": "powerline",
                    "background": "p:docker",
                    "foreground": "p:docker-fg",
                    "template": "\uE7B0 {{ if not .Error }}{{ .Context }}{{ end }} "
                },
                {
                    "type": "angular",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-angular",
                    "foreground": "p:lang-angular-fg",
                    "template": " \uE753 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "cds",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-cds",
                    "foreground": "p:lang-cds-fg",
                    "template": "\uE311 CDS {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "cmake",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-cmake",
                    "foreground": "p:lang-cmake-fg",
                    "template": "\uE61E \uE61D Cmake {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "crystal",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-crystal",
                    "foreground": "p:lang-crystal-fg",
                    "template": "\uE62F {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "dart",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-dart",
                    "foreground": "p:lang-dart-fg",
                    "template": " \uE798 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-dotnet",
                    "foreground": "p:lang-dotnet-fg",
                    "template": " \uE70C {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "flutter",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-flutter",
                    "foreground": "p:lang-flutter-fg",
                    "template": " \uE28E {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "go",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-go",
                    "foreground": "p:lang-go-fg",
                    "template": " \uFCD1 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "haskell",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-haskell",
                    "foreground": "p:lang-haskell-fg",
                    "template": " \uE61F {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "java",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-java",
                    "foreground": "p:lang-java-fg",
                    "template": " \uE738 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "julia",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-julia",
                    "foreground": "p:lang-julia-fg",
                    "template": " \uE624 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "kotlin",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-kotlin",
                    "foreground": "p:lang-kotlin-fg",
                    "template": " \uE634 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "kubectl",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-kubectl",
                    "foreground": "p:lang-kubectl-fg",
                    "template": "\uFD31 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "lua",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-lua",
                    "foreground": "p:lang-lua-fg",
                    "template": "\uE620 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-node",
                    "foreground": "p:lang-node-fg",
                    "properties": {
                        "fetch_package_manager": true,
                        "yarn_icon": "<p:lang-yarn>\uF61A</>",
                        "npm_icon": "<p:lang-npm>\uE71E</>"
                    },
                    "template": " \uE718{{ if .PackageManagerIcon }} {{ .PackageManagerIcon }}{{ end }} {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "nx",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-nx",
                    "foreground": "p:lang-nx-fg",
                    "template": "NX {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "perl",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-perl",
                    "foreground": "p:lang-perl-fg",
                    "template": "\uE769 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "php",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-php",
                    "foreground": "p:lang-php-fg",
                    "template": " \uE73D {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-python",
                    "foreground": "p:lang-python-fg",
                    "template": " \uE235 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "r",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-r",
                    "foreground": "p:lang-r-fg",
                    "template": " \uFCD2 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "ruby",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-ruby",
                    "foreground": "p:lang-ruby-fg",
                    "template": " \uE791 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "rust",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-rust",
                    "foreground": "p:lang-rust-fg",
                    "template": " \uE7A8 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "swift",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "p:lang-swift",
                    "foreground": "p:lang-swift-fg",
                    "template": " \uE755 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "executiontime",
                    "style": "diamond",
                    "background": "p:code-ok",
                    "foreground": "p:light-fg",
                    "leading_diamond": "<transparent,background>\uE0B0</>",
                    "trailing_diamond": "\uE0B0",
                    "properties": {
                        "style": "austin",
                        "threshold": 500,
                        "always_enabled": true
                    },
                    "template": " \uE384 {{ .FormattedMs }} "
                },
                {
                    "type": "exit",
                    "style": "diamond",
                    "background": "p:code-ok",
                    "foreground": "p:light-fg",
                    "leading_diamond": "<transparent,background>\uE0B0</>",
                    "trailing_diamond": "\uE0B0",
                    "background_templates": [
                        "{{ if gt .Code 0 }}p:code-ko{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "template": "{{ if .Root }}\uF922{{else}} {{end}}\uF054 "
                }
            ],
            "type": "prompt"
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "p:terminal-other",
        "foreground_templates": [
            "{{ if eq .Shell \"bash\" }}p:terminal-bash{{end}}",
            "{{ if eq .Shell \"powershell\" }}p:terminal-powershell{{end}}",
            "{{ if eq .Shell \"pwsh\" }}p:terminal-pwsh{{end}}",
            "{{ if eq .Shell \"cmd\" }}p:terminal-cmd{{end}}"
        ],
        "template": "{{ if .Segments.Executiontime.FormattedMs }}{{ .Segments.Executiontime.FormattedMs }}{{else}}0ms{{end}} {{ if gt .Code 0 }}<p:code-ko>\uF00D</>{{else}}<p:code-ok>\uF00C</>{{end}} <p:light-fg>\uF054</> "
    },
    "version": 2,
    "final_space": true
}