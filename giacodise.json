{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "diamond",
                    "background": "#40739E",
                    "foreground": "#FEFEFE",
                    "leading_diamond": "\uE0B6",
                    "trailing_diamond": "\uE0B0",
                    "properties": {
                        "style": "folder"
                    },
                    "template": " {{ .Path }} "
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "background": "#FFFB38",
                    "foreground": "#1C1C1D",
                    "powerline_symbol": "\uE0B0",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB95{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#C5E478{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#C792EA{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "\uE725 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_worktree_count": true,
                        "truncate_symbol": "...",
                        "branch_max_length": 25
                    },
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#a7484b>\uEA73 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#095d48>\uF62B {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} "
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "background": "#0F9170",
                    "foreground": "#FEFEFE",
                    "powerline_symbol": "\uE0B0",
                    "properties": {
                        "fetch_package_manager": true
                    },
                    "template": " \uE718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }} {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#822CC4",
                    "foreground": "#FEFEFE",
                    "template": " \uE70C {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "java",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#3A75B0",
                    "foreground": "#FEFEFE",
                    "template": " \uE738 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "php",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#8892BF",
                    "foreground": "#FEFEFE",
                    "template": " \uE73D {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "angular",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#D81233",
                    "foreground": "#FEFEFE",
                    "template": " \uE753 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "dart",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#03589C",
                    "foreground": "#FEFEFE",
                    "template": " \uE798 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "flutter",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#60C9F8",
                    "foreground": "#FEFEFE",
                    "template": " \uE28E {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "go",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#79D4FD",
                    "foreground": "#FEFEFE",
                    "template": " \uFCD1 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "haskell",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#313131",
                    "foreground": "#5E5086",
                    "template": " \uE61F {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "julia",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#4063D8",
                    "foreground": "#FEFEFE",
                    "template": " \uE624 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "r",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#C1C2C5",
                    "foreground": "#2A64B7",
                    "template": " \uFCD2 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "ruby",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#CC342D",
                    "foreground": "#FEFEFE",
                    "template": " \uE791 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "rust",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#99908A",
                    "foreground": "#193549",
                    "template": " \uE7A8 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "swift",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#F43B26",
                    "foreground": "#FEFEFE",
                    "template": " \uE755 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#326B9B",
                    "foreground": "#FEFEFE",
                    "template": " \uE235 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "kotlin",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "background": "#27282C",
                    "foreground": "#774AEC",
                    "template": " \uE634 {{ if not .Error }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "executiontime",
                    "style": "diamond",
                    "background": "#1FA07D",
                    "foreground": "#FEFEFE",
                    "leading_diamond": "<transparent,background>\uE0B0</>",
                    "trailing_diamond": "\uE0B0",
                    "properties": {
                        "always_enabled": true,
                        "style": "austin",
                        "threshold": 500
                    },
                    "template": " \uE384 {{ .FormattedMs }} "
                },
                {
                    "type": "exit",
                    "style": "diamond",
                    "background": "#1FA07D",
                    "foreground": "#FEFEFE",
                    "leading_diamond": "<transparent,background>\uE0B0</>",
                    "trailing_diamond": "\uE0B0",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#EE5253{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "template": " \uF054 "
                }
            ],
            "type": "prompt"
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground_templates": [
            "{{ if eq .Shell \"bash\" }}#DF9395{{end}}",
            "{{ if eq .Shell \"powershell\" }}#61AFEF{{end}}",
            "{{ if eq .Shell \"pwsh\" }}#61AFEF{{end}}",
            "{{ if eq .Shell \"cmd\" }}#B2BEC3{{end}}"
        ],
        "template": ">> "
    },
    "version": 2,
    "final_space": true
}